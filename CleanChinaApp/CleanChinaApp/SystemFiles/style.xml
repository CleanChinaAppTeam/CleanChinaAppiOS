<?xml version="1.0" encoding="UTF-8"?>
  <!--说明 
   保留字:
   name:样式属性名称
   type:类型
   value:值
   parent:继承自
   -->
<Style>
    <!--重度的字体-->
    <lablargedark>
        <property name="font"       type="UIFont"   value="FontScaleLarge"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleDark" />
    </lablargedark>
    <lablargemilk>
        <property name="font"       type="UIFont"   value="FontScaleLarge"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleMilkWhite" />
    </lablargemilk>
    <lablargedarkbold>
        <property name="font"       type="UIFont"   value="FontScaleLargeBold"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleDark" />
    </lablargedarkbold>

    <labmiddark>
        <property name="font"       type="UIFont"   value="FontScaleMid"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleDark" />
        <property name="highlightedTextColor"  type="UIColor"  value="ColorStyleMilkWhite" />
        
        
    </labmiddark >
    <labmidlight>
        <property name="font"       type="UIFont"   value="FontScaleMid"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleLight" />
        <property name="highlightedTextColor"  type="UIColor"  value="ColorStyleMilkWhite" />
        
        
    </labmidlight >
    <labmiddarknohighlight parent = "labmiddark">
        <property name="highlightedTextColor"  type="UIColor"  value="ColorStyleDark" />
        
        
    </labmiddarknohighlight>
    <labmindark>
        <property name="font"       type="UIFont"   value="FontScaleSmall"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleDark" />
        <property name="highlightedTextColor"  type="UIColor"  value="ColorStyleMilkWhite" />
        
    </labmindark>
      <!--轻便的文本结构-->
    <labminlight>
        <property name="font"       type="UIFont"   value="FontScaleSmall"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleLight" />
    </labminlight>
    <labminmilk>
        <property name="font"       type="UIFont"   value="FontScaleSmall"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleMilkWhite" />
    </labminmilk>
    <labmidmilk>
        <property name="font"       type="UIFont"   value="FontScaleMid"/>
        <property name="textColor"  type="UIColor"  value="ColorStyleMilkWhite" />
    </labmidmilk>
       <!--标准样式button-->
    <btnstandard>
        <property name="font"       type="UIFont"   value="FontScaleSmallBold"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleWhite" externType = "int"  extern = "0"/>
        <property name="backgroundImage:forState" type="UIImage.Stretchable" value="bg_button_standard" externType = "int"  extern = "0"/>
    </btnstandard>
    <btnsegmented>
        <property name="font"       type="UIFont"   value="FontScaleSmallBold"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleWhite" externType = "int"  extern = "0"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "2"/>
        <property name="backgroundImage:forState" type="UIImage.Stretchable" value="bg_button_standard" externType = "int"  extern = "0"/>
        <property name="backgroundImage:forState" type="UIImage.Stretchable" value="bg_null" externType = "int"  extern = "1"/>
        <property name="backgroundImage:forState" type="UIImage.Stretchable" value="bg_null" externType = "int"  extern = "2"/>
        
    </btnsegmented>
    <btnstandardmid >
        <property name="font"       type="UIFont"   value="FontScaleMid"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "0"/>
        
    </btnstandardmid>
    <btndropdownmid parent ="btnstandardmid">
        <property name="image:forState" type="UIImage" value="ic_arrow_down" externType = "int"  extern = "0"/>
        
    </btndropdownmid>
    
   
    <btnstandardmin >
        <property name="font"       type="UIFont"   value="FontScaleSmall"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleDark" externType = "int"  extern = "0"/>
    </btnstandardmin>
    <btnnavigation >
        <property name="font"       type="UIFont"   value="FontScaleSmallBold"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleWhite" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleWhite" externType = "int"  extern = "0"/>
    </btnnavigation>
    <btnmilkwhite>
        <property name="font"       type="UIFont"   value="FontScaleSmall"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleMilkWhite" externType = "int"  extern = "1"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleMilkWhite" externType = "int"  extern = "0"/>
    </btnmilkwhite>

    <btnsubmit parent = "btnstandard">
         <property name="font"       type="UIFont"   value="FontScaleLarge"/>
        <property name="backgroundImage:forState" type="UIImage.Stretchable" value="btn_bg_submit" externType = "int"  extern = "0"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleMilkWhite" externType = "int"  extern = "0"/>
        <property name="titleColor:forState" type="UIColor" value="ColorStyleMilkWhite" externType = "int"  extern = "1"/>
    </btnsubmit>
    <!--login-->
    <lablogintitle parent = "labmiddark">
        <property name="font"       type="UIFont"   value="FontScaleSupereBold"/>
        <property name="textColor"  type="UIColor"  value="ColorSelected" />
        <property name="textAlignment"  type="int"  value="1" />
    </lablogintitle>

    
    <!--顶部-->
  
</Style>